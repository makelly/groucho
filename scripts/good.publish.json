{
  "title": "Good publish script",
  "description": "This is an example of a good, no errors, script for publishing events",
  "version": "03/08/2018",
  "publisher": "test.publisher.data.json",

  "data": [
    {"name": "providers", "list": ["test.provider.data.json"]},
    {"name": "encounters", "list": ["test.encounter.data.json"]},
    {"name": "patients", "list": ["test.smith.data.json", "test.frederikson.data.json", "test.crowther.data.json"]},
    {"name": "immunizations", "list": ["test.immunization01.data.json", "test.immunization02.data.json"]},
    {"name": "medications", "list": ["test.medication01.data.json", "test.medication02.data.json", "test.medication03.data.json"]}
  ],

  "events": [
    {"template": "immunization.beta.template.json", "type": "json", "eventData": "immunizations"},
    {"template": "medication.beta.template.xml", "type": "xml", "eventData": "medications"}
  ],

  "PUBLISH": {
    "FOR_EACH_PATIENT": "patients",
    "FOR_EACH_PROVIDER": "providers",
    "FOR_EACH_ENCOUNTER": "encounters"
  }
}
