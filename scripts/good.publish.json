{
  "title": "Good publish script",
  "description": "This is an example of a good, no errors, script for publishing events",
  "version": "03/08/2018",
  "publisher": "test.publisher.data.json",

  "data": [
    {"name": "providers", "list": ["test.provider.data.json"]},
    {"name": "encounters", "list": ["test.encounter.data.json"]},
    {"name": "patients", "list": ["test.smith.patient.data.json", "test.frederikson.patient.data.json", "test.crowther.patient.data.json"]},
    {"name": "immunizations", "list": ["test.immunization01.event.data.json", "test.immunization02.event.data.json"]},
    {"name": "medications", "list": ["test.medication01.event.data.json", "test.medication02.event.data.json", "test.medication03.event.data.json"]}
  ],

  "events": [
    {"template": "immunization.beta.template.json", "type": "CH015", "format": "json", "data": "immunizations"},
    {"template": "medication.beta.template.xml", "type": "CH019", "format": "xml", "data": "medications"}
  ],

  "PUBLISH": {
    "FOR_EACH_PATIENT": "patients",
    "FOR_EACH_PROVIDER": "providers",
    "FOR_EACH_ENCOUNTER": "encounters"
  }
}
