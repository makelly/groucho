{
  "name": "Example 1 publish script",
  "description": "This is an example script for publishing events.",
  "version": "03/08/2018",
  "publisher": "test.publisher.data.json",

  "data": [
    {"name": "providers", "items": ["test.provider.data.json"]},
    {"name": "encounters", "items": ["test.encounter.data.json"]},
    {"name": "patients", "items": ["test.smith.data.json", "test.frederikson.data.json", "test.crowther.data.json"]},
    {"name": "immunizations", "items": ["test.immunization01.data.json", "test.immuninzation02.data.json"]},
    {"name": "medications", "items": ["test.medication01.data.json", "test.medication02.data.json", "test.medication03.data.json"]}
  ],

  "templates": [
    {"template": "immunization.template.json", "type": "json", "eventData": "immunizations"},
    {"template": "medication.template.xml", "type": "xml", "eventData": "medications"}
  ],

  "#PUBLISH": {
    "#FOR_EACH_PATIENT": "patients",
    "#FOR_EACH_PROVIDER": "providers",
    "#FOR_EACH_ENCOUNTER": "encounters"
  }
}
